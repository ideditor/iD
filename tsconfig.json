{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "listEmittedFiles": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true
  },
}